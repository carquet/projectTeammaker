

<p id="notice"><%= notice %></p>
<h1>Booking of the Month</h1>
	<div class="row">
	    <div class="col-md-6">
			<%= month_calendar do |date| %>
			<div class="date"><%= date %></div>
				<% end %>
		</div>

		<div class="row">
		    <div class="col-md-4">
				<p id= "js-available-employee"></p>
			</div>
		</div>	
		

		<div class="js-booking col-md-3">
			<h4 id="js-title">Booking on this date: </h4>
			<p id="js-booking_of_the_day"></p>
		</div>


		<div class="col-md-3">
			<h4>Employees working on that day </h4>
				<p id="js-employees">	
				</p>
		</div>
	
</div>





	